<div class="container border">
  <h1 class="text-info" style="font-size: 80px; padding: 20px; "> TANDEM </h1>
  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">Project ID</th>
        <th scope="col">Developers</th>
        <th scope="col">Sprints</th>
        <th scope="col">Created At</th>
        <th scope="col">Actions</th>
      </tr>
    </thead>
    <tbody>
    <% @projects.each do |project| %>
      <tr>
        <th scope="row"><%= project.id %></th>
        <td><%= project.users.count %></td>
        <td><%= project.sprints %></td>
        <td><%= project.created_at.strftime("%F") %></td>
        <td ><%= link_to "Show", project, class:"text-info"%></td>

      </tr>
    <% end %>
    </tbody>
  </table>
  <div class="digg_pagination my-5">
    <%= will_paginate @projects %>
  </div>









</div>

